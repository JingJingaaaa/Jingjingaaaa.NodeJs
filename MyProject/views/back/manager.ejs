        <table width="100%" border="0" cellpadding="8" cellspacing="0" class="tableBasic">
     <tr>
      <th width="30" align="center">编号</th>
      <th align="left">管理员名称</th>
      <th align="center">密码</th>
      <th align="center">添加时间</th>
      <th align="center">操作</th>
     </tr>
     <% for(var i=0;i<managers.length;i++){ %>
         <tr>
          <td align="center"><%= i+1 %></td>
          <td><%= managers[i].username %></td>
          <td align="center"><%= managers[i].password %></td>
          <td align="center"><%= managers[i].create_date %></td>
          <td align="center"><a href="/admin/Medit?_id=<%= managers[i]._id %>">编辑</a> | <a href="/admin/Mdelete?username=<%= managers[i].username %>">删除</a></td>
         </tr>
    <% } %>
